@model FormSubmitApp.Models.FinanceRegistration

<h2>Registration Details</h2>
<p><strong>Full Name:</strong> @Model.FullName</p>
<p><strong>Email:</strong> @Model.Email</p>
<p><strong>Phone Number:</strong> @Model.PhoneNumber</p>
<p><strong>Date of Birth:</strong> @Model.DateOfBirth.ToString("d")</p>
<p><strong>Income:</strong> @Model.Income:C</p>
<p><strong>Budget:</strong> @Model.Budget:C</p>
<p><strong>Employment Status:</strong> @Model.EmploymentStatus</p>
<p><strong>Currency:</strong> @Model.Currency</p>
